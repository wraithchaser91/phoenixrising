<%formatDate=timestamp=>{let date = new Date(timestamp).toISOString().split("T")[0];let pieces=date.split("-");return`${pieces[2]}/${pieces[1]}/${pieces[0]}`;}%>
<%formatDateWithTimes=timestamp=>{let date = new Date(timestamp).toISOString();let datePieces=date.split("T")[0];let pieces=datePieces.split("-");return`${pieces[2]}/${pieces[1]}/${pieces[0]} ${date.split("T")[1].split(".")[0]}`;}%>
<%findTimeDifference=(date1,date2)=>{let day1=formatDate(date1).substring(0,2);let day2=formatDate(date2).substring(0,2);return parseInt(day1)-parseInt(day2);}%>